{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "bs5-controller-ui",
	"version": "0.1.0",
	"identifier": "com.beolyd5.dev",
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devUrl": "http://localhost:1421/Beolyd5/",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"fullscreen": false,
				"resizable": false,
				"title": "bs5-controller",
				"width": 1024,
				"height": 768
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"shell": {
			"open": true
		}
	}
}
